{
  "name": "accessibility-strapi",
  "private": true,
  "version": "0.1.0",
  "description": "A Strapi application",
  "scripts": {
    "develop": "strapi develop",
    "meilisearch:download": "curl -L https://install.meilisearch.com | sh && mkdir .tmp 2> /dev/null || mv meilisearch .tmp/",
    "meilisearch:do-not-use-directly": ".tmp/meilisearch",
    "develop:withsearch": "concurrently --names \"MEILISEARCH,STRAPI\" -c \"blue.bold,magenta.bold\" \"dotenv-run-script meilisearch:do-not-use-directly\" \"strapi develop\"",
    "start": "strapi start",
    "build": "strapi build",
    "strapi": "strapi"
  },
  "dependencies": {
    "@ckeditor/strapi-plugin-ckeditor": "github:strathos/strapi-plugin-ckeditor",
    "@strapi/plugin-documentation": "^4.14.3",
    "@strapi/plugin-graphql": "^4.7.1",
    "@strapi/plugin-i18n": "^4.8.2",
    "@strapi/plugin-users-permissions": "^4.8.2",
    "@strapi/strapi": "^4.8.2",
    "pg": "^8.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^5.3.4",
    "strapi-plugin-meilisearch": "^0.10.0",
    "strapi-plugin-navigation": "^2.2.4",
    "strapi-plugin-sitemap": "^3.0.3",
    "strapi-plugin-vercel-deploy": "^1.4.0",
    "styled-components": "^5.3.11"
  },
  "devDependencies": {
    "better-sqlite3": "^9.2.2",
    "concurrently": "^8.0.1",
    "dotenv-run-script": "^0.4.1",
    "eslint": "^8.56.0"
  },
  "author": {
    "name": "A Strapi developer"
  },
